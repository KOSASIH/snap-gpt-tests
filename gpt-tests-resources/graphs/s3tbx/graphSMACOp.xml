<graph id="GraphSMACOp">
    <version>1.0</version>
    <node id="Read">
        <operator>Read</operator>
        <sources/>
        <parameters>
            <file>$input</file>
        </parameters>
    </node>

    <node id="Subset">
        <operator>Subset</operator>
        <sources>
            <sourceProduct refid="Read"/>
        </sources>
        <parameters>
            <region>800,3600,200,200</region>
            <copyMetadata>false</copyMetadata>
        </parameters>
    </node>

    <node id="SMAC">
        <operator>SmacOP</operator>
        <sources>
            <sourceProduct refid="Subset"/>
        </sources>
        <parameters>
            <tauAero550>0.2</tauAero550>
            <uH2o>3.0</uH2o>
            <uO3>0.15</uO3>
            <surfPress>1013.0</surfPress>
            <useMerisADS>true</useMerisADS>
            <aerosolType>DESERT</aerosolType>
            <invalidPixel>0.0</invalidPixel>
            <maskExpression>not (l1_flags.INVALID or l1_flags.BRIGHT)</maskExpression>
            <maskExpressionForward>not used</maskExpressionForward>
            <bandNames>radiance_1,radiance_2,radiance_3,radiance_4,radiance_5,radiance_6,radiance_7,radiance_8,radiance_9,radiance_10,radiance_11,radiance_12,radiance_13,radiance_14,radiance_15</bandNames>
        </parameters>
    </node>

    <node id="Write">
        <operator>Write</operator>
        <sources>
            <sourceProduct refid="SMAC"/>
        </sources>
        <parameters>
            <file>$output</file>
            <formatName>BEAM-DIMAP</formatName>
        </parameters>
    </node>
    <applicationData id="Presentation">
        <Description/>
        <node id="Read">
            <displayPosition x="20.0" y="136.0"/>
        </node>
        <node id="Subset">
            <displayPosition x="140.0" y="136.0"/>
        </node>
        <node id="SMAC">
            <displayPosition x="260.0" y="136.0"/>
        </node>
        <node id="Write">
            <displayPosition x="380.0" y="136.0"/>
        </node>
    </applicationData>

</graph>

