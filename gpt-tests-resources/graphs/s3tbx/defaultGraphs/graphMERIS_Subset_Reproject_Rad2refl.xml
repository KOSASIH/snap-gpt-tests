<graph id="GraphMERIS_Subset_Reproject_Rad2refl">
    <version>1.0</version>

    <node id="Read">
        <operator>Read</operator>
        <sources/>
        <parameters>
        <file>$input1</file>
        </parameters>
    </node>

    <node id="Subset">
        <operator>Subset</operator>
        <sources>
            <sourceProduct refid="Read"/>
        </sources>
        <parameters>
            <sourceBands/>
            <region>0,0,0,0</region>
            <geoRegion/>
            <subSamplingX>1</subSamplingX>
            <subSamplingY>1</subSamplingY>
            <fullSwath>false</fullSwath>
            <tiePointGridNames/>
            <copyMetadata>true</copyMetadata>
        </parameters>
    </node>

    <node id="Reproject">
        <operator>Reproject</operator>
        <sources>
            <sourceProduct refid="Subset"/>
        </sources>
        <parameters>
            <wktFile/>
            <crs>EPSG:4326</crs>
            <resampling>Nearest</resampling>
            <referencePixelX/>
            <referencePixelY/>
            <easting/>
            <northing/>
            <orientation/>
            <pixelSizeX/>
            <pixelSizeY/>
            <width/>
            <height/>
            <tileSizeX/>
            <tileSizeY/>
